{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/svelte/build",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*"
  ],
  "modes": {
    "window": {
      "title": "neudeck",
      "width": 800,
      "height": 500,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/www/icon.png"
    }
  },
  "cli": {
    "binaryName": "neudeck",
    "resourcesPath": "/svelte/build",
    "extensionsPath": "/extensions/",
    "binaryVersion": "4.10.0",
    "clientVersion": "3.8.2"
  }
}